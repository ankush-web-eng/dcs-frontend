{
  "name": "dcs-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --watch --poll=2000 --port=4200 --host=0.0.0.0 --disable-host-check",
    "start:admin": "ng serve dcs-admin --watch --port=4201 --poll=2000 --host=0.0.0.0 --disable-host-check --base-href /dashboard/",
    "build": "ng build",
    "build:admin": "ng build dcs-admin --configuration=dev --base-href /dashboard/",
    "build:admin:prod": "ng build dcs-admin --configuration production --base-href /dashboard/",
    "test": "ng test --code-coverage",
    "lint": "nx workspace-lint && ng lint",
    "serve:ssr": "node dist/server",
    "build:ssr": "ng build --configuration production && ng run dcs-frontend:server:production --bundleDependencies all && npm run compile:server",
    "build:ssr:dev": "ng build --configuration=dev && ng run dcs-frontend:server:production --bundleDependencies all && npm run compile:server",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "nx": "nx",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "ng update @nrwl/workspace",
    "update:check": "ng update",
    "dep-graph": "nx dep-graph",
    "workspace-schematic": "nx workspace-schematic",
    "help": "nx help"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.7",
    "@angular/cdk": "^12.2.7",
    "@angular/common": "~12.2.7",
    "@angular/compiler": "~12.2.7",
    "@angular/core": "~12.2.7",
    "@angular/flex-layout": "^9.0.0-beta.31",
    "@angular/forms": "~12.2.7",
    "@angular/material": "^12.2.7",
    "@angular/platform-browser": "~12.2.7",
    "@angular/platform-browser-dynamic": "~12.2.7",
    "@angular/platform-server": "~12.2.7",
    "@angular/router": "~12.2.7",
    "@nguniversal/express-engine": "12.1.0",
    "@nguniversal/module-map-ngfactory-loader": "v8.2.6",
    "@ngxs/devtools-plugin": "^3.7.2",
    "@ngxs/schematics": "0.0.1-alpha.5",
    "@ngxs/storage-plugin": "^3.7.2",
    "@ngxs/store": "^3.7.2",
    "@nrwl/angular": "^12.9.0",
    "@nrwl/schematics": "^8.12.7",
    "apollo-angular": "^1.8.0",
    "apollo-angular-link-http": "^1.9.0",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link": "^1.2.13",
    "core-js": "^2.5.4",
    "express": "^4.15.2",
    "graphql": "^14.6.0",
    "graphql-tag": "^2.10.3",
    "moment": "^2.24.0",
    "ng-lazyload-image": "^9.1.0",
    "ngx-chips": "^2.2.2",
    "ngx-clipboard": "^14.0.1",
    "ngx-markdown": "^12.0.1",
    "rxjs": "^6.5.4",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.7",
    "@angular/cli": "^12.2.7",
    "@angular/compiler-cli": "~12.2.7",
    "@angular/language-service": "~12.2.7",
    "@nrwl/jest": "^12.9.0",
    "@nrwl/workspace": "^12.9.0",
    "@types/express": "^4.17.0",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jest": "^24.9.0",
    "jest-preset-angular": "^8.0.0",
    "pre-push": "^0.1.1",
    "prettier": "2.4.1",
    "protractor": "~7.0.0",
    "ts-jest": "^27.0.5",
    "ts-loader": "^5.2.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.3.5",
    "webpack-cli": "^3.3.11"
  },
  "pre-push": {
    "run": [
      "lint",
      "test"
    ]
  }
}
