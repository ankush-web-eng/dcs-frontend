{
  "name": "dcs-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --watch --poll=2000 --port=4200 --host=0.0.0.0 --disable-host-check",
    "start:admin": "ng serve dcs-admin --watch --port=4201 --poll=2000 --host=0.0.0.0 --disable-host-check --base-href /dashboard/",
    "build": "ng build",
    "build:admin": "ng build dcs-admin --configuration=dev --base-href /dashboard/",
    "build:admin:prod": "ng build dcs-admin --prod --base-href /dashboard/",
    "test": "ng test --code-coverage",
    "lint": "nx workspace-lint && ng lint",
    "serve:ssr": "node dist/server",
    "build:ssr": "ng build --prod && ng run dcs-frontend:server:production --bundleDependencies all && npm run compile:server",
    "build:ssr:dev": "ng build --configuration=dev && ng run dcs-frontend:server:production --bundleDependencies all && npm run compile:server",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "nx": "nx",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "ng update @nrwl/workspace",
    "update:check": "ng update",
    "dep-graph": "nx dep-graph",
    "workspace-schematic": "nx workspace-schematic",
    "help": "nx help"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.14",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/flex-layout": "^8.0.0-beta.27",
    "@angular/forms": "~8.2.14",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/platform-server": "~8.2.14",
    "@angular/router": "~8.2.14",
    "@nguniversal/express-engine": "v8.2.6",
    "@nguniversal/module-map-ngfactory-loader": "v8.2.6",
    "@ngxs/devtools-plugin": "^3.6.2",
    "@ngxs/schematics": "0.0.1-alpha.5",
    "@ngxs/storage-plugin": "^3.6.2",
    "@ngxs/store": "^3.6.2",
    "@nrwl/angular": "^9.0.2",
    "@nrwl/schematics": "^8.12.7",
    "apollo-angular": "^1.8.0",
    "apollo-angular-link-http": "^1.9.0",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link": "^1.2.13",
    "core-js": "^2.5.4",
    "express": "^4.15.2",
    "graphql": "^14.6.0",
    "graphql-tag": "^2.10.3",
    "hammerjs": "^2.0.8",
    "moment": "^2.24.0",
    "ng-lazyload-image": "^7.0.1",
    "ngx-chips": "^2.1.0",
    "ngx-clipboard": "^12.3.1",
    "ngx-markdown": "^8.2.2",
    "rxjs": "^6.5.4",
    "tslib": "^1.11.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.29",
    "@angular/cli": "^8.3.29",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@nrwl/jest": "^9.0.2",
    "@nrwl/workspace": "^9.0.2",
    "@types/express": "^4.17.0",
    "@types/jest": "^24.9.1",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.2.1",
    "jest": "^24.9.0",
    "jest-preset-angular": "^8.0.0",
    "pre-push": "^0.1.1",
    "prettier": "1.18.2",
    "protractor": "^5.4.3",
    "ts-jest": "24.0.0",
    "ts-loader": "^5.2.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3",
    "webpack-cli": "^3.3.11"
  },
  "pre-push": {
    "run": [
      "lint",
      "test"
    ]
  }
}
